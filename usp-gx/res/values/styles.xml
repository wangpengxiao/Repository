<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="title_text">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="delivery_history_text">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">left</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="VanclUpdateDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/panel_background</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">false</item>
    </style>

    <style name="complaint_suggest_item_textStyle">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="app_layout_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/list_item_bg</item>
    </style>

    <style name="app_layout_top_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/list_item_top_bg</item>
    </style>

    <style name="app_layout_middle_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/list_item_middle_bg</item>
    </style>

    <style name="app_layout_bottom_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@drawable/list_item_bottom_bg</item>
    </style>

    <style name="app_textview_left_style" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="app_textview_right_style" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="app_edittext_style" parent="@android:style/Widget.EditText">
        <item name="android:background">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:width">150dp</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@android:color/darker_gray</item>
    </style>

    <style name="app_arrow_right_style">
        <item name="android:layout_width">8dp</item>
        <item name="android:layout_height">13dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:background">@drawable/arrow_more</item>
    </style>

    <style name="app_editimage_style">
        <item name="android:layout_width">13dp</item>
        <item name="android:layout_height">13dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">13dp</item>
        <item name="android:background">@drawable/eidt</item>
    </style>

    <style name="list_view_item_first">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:bufferType">spannable</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">#626970</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:shadowColor">@color/white</item>
    </style>

    <style name="list_view_item_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:bufferType">spannable</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">#626970</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:shadowColor">@color/white</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="list_view_key">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:bufferType">spannable</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:shadowColor">@color/white</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="table_row_style">
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>

    <style name="table_row_style_grid">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="image_view_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">6dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="image_view_style_grid">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">12dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="actionbar_common_image_button_style">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:padding">13dp</item>
        <item name="android:background">@drawable/usp_topbar_btn_selector</item>
    </style>

    <style name="actionbar_common_text_button_style">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:textColor">#000000 </item>
        <item name="android:textSize">18sp </item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/actionbar_common_btn_selector</item>
    </style>

    <style name="table_row_style_channel">
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
    </style>

    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
    </style>

    <style name="PopupAnimationDownup" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popup_downup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_downup_exit</item>
    </style>

    <style name="grid_item_style">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:textColor">#333333 </item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/grid_item_selector</item>
    </style>

    <style name="usp_left_drawer_item_layout">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#484848</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/open_btn_selector</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">40.0dip</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:minHeight">50.0dip</item>
    </style>

    <style name="usp_left_drawer_count" parent="@style/usp_update_count">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#9d9e9e</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">25.0dip</item>
        <item name="android:layout_marginRight">5.0dip</item>
    </style>

    <style name="usp_checkbox_style" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/usp_checkbox_selector</item>
        <item name="android:textColor">@color/font_6d</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingLeft">10.0dip</item>
    </style>

    <style name="navigation_drawer_item_line">
        <item name="android:background">@drawable/usp_navigation_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="navigation_drawer_item_layout">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/usp_navigation_drawer_item_bg</item>
        <item name="android:paddingLeft">20.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">50.0dip</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="navigation_drawer_item_text">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/navigation_drawer_item_text</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">10.0dip</item>
    </style>

    <style name="navigation_drawer_title">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/navigation_font_title</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="navigation_drawer_text">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/navigation_font_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="navigation_line_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48dip</item>
    </style>

    <style name="navigation_line_text_layout">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">20dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dip</item>
    </style>

    <style name="usp_update_count">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">#ffcc3131</item>
        <item name="android:gravity">center</item>
        <item name="android:height">15.0dip</item>
        <item name="android:width">15.0dip</item>
    </style>

    <style name="usp_btn_common_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48dip</item>
        <item name="android:layout_marginLeft">25dip</item>
        <item name="android:layout_marginRight">25dip</item>
        <item name="android:layout_marginBottom">10dip</item>
