                    style="@style/app_textview_left_style"
                    android:text="2G活跃用户" />

                <TextView
                    android:id="@+id/g2_activityusernum"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="2G欠费停机用户" />

                <TextView
                    android:id="@+id/g2_overdueusernum"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="2G流失用户" />

                <TextView
                    android:id="@+id/g2_lossusernum"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="2G收入" />

                <TextView
                    android:id="@+id/g2_income"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="2G欠费" />

                <TextView
                    android:id="@+id/g2_arrears"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>
            <!-- 2G结束，3G开始 -->

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="3G新增用户" />

