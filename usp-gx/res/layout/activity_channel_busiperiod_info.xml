                    android:text="宽带欠费" />

                <TextView
                    android:id="@+id/broadband_arrears"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>
            <!-- 宽带结束 ,佣金开始 -->

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="一次佣金" />

                <TextView
                    android:id="@+id/commission_one"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
