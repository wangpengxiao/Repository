                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="3G欠费" />

                <TextView
                    android:id="@+id/g3_arrears"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>
            <!-- 3G结束，固话开始 -->

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="固话新增用户" />

                <TextView
                    android:id="@+id/fix_addusernum"
                    style="@style/app_textview_right_style"
                    android:text="空"
                    android:textColor="#49B41E" />
            </RelativeLayout>

            <RelativeLayout style="@style/app_layout_middle_style" >

                <TextView
                    style="@style/app_textview_left_style"
                    android:text="固话活跃用户" />

                <TextView
                    android:id="@+id/fix_activityusernum"
